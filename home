<script type="text/javascript">
        function sflights() {
            $("#banner").addClass('bgfl');
            $("#banner").removeClass('bgbus');
            $("#banner").removeClass('bgcab');
            $("#banner").removeClass('bghd');
            $("#banner").removeClass('bght');
            $("#banner").removeClass('bgmb');

            $("#dspf").show();
            $("#dsph").hide();
            $("#dspr").hide();
            $("#dspb").hide();
            $("#dspc").hide();
            $("#dsphp").hide();
            $("#ftab").addClass('active');
            $('#htab').removeClass('active');
            $("#rtab").removeClass('active');
            $('#btab').removeClass('active');
            $('#ctab').removeClass('active');
            $('#hptab').removeClass('active');
            $('#lihome').removeClass('active');
            $("#ftab1").addClass('active');
            $('#htab1').removeClass('active');
            $('#btab1').removeClass('active');
            $('#ctab1').removeClass('active');
            $('#hptab1').removeClass('active');
        }
