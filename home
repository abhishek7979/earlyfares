        $('#aDomesticf').click(function () {

            $('#aInternationalf').removeClass('active');
            $('#aDomesticf').addClass('active');
            $('#aRound').removeClass('active');
            $('#aOneway').addClass('active');
            $('#toneway').addClass('active');
            $('#tround').removeClass('active');

            $('#rbtnfdom').attr({ checked: true });
            $('#hdnSelectedSource').val('');
            $('#hdnSelectedDestination').val('');
            $('#txtffcb').val('');
            $('#txtftcb').val('');
            $('#aspecial').show();


        });
        $('#aInternationalf').click(function () {
            $('#aspecial').hide();
            $('#aDomesticf').removeClass('active');
            $('#aInternationalf').addClass('active');
            $('#aRound').removeClass('active');
            $('#aOneway').addClass('active');
            $('#tround').removeClass('active');
            $('#toneway').addClass('active');
            $('#rbtnfint').attr({ checked: true });
            $('#txtffcb').val('');
            $('#txtftcb').val('');

            if ($('input[name=trip]:checked').attr("id") == "ROUND") {
                $('#rbtnfrt').attr({ checked: true });
                $('#aRound').addClass('radiobnsselected');
            } else {
                $('#rbtnfst').attr({ checked: true });
                $('#aOneway').addClass('radiobnsselected');
                $('#frtn').hide();
            }
            $('#hdnSelectedSource').val('');
            $('#hdnSelectedDestination').val('');
            $('#txtDestination').val('');
            $('#txtSource').val('');
        });

        $('#aOneway').click(function () {

            $('#aRound').removeClass('active');
            $('#aspecial').removeClass('active');
            $('#aOneway').addClass('active');
            $('#rbtnfst').attr({ checked: true });
            $('#frtn').hide();
        });
        $('#aRound').click(function () {
            $('#aOneway').removeClass('active');
            $('#aspecial').removeClass('active');
            $('#aRound').addClass('active');
            $('#frtn').show();
            $('#rbtnfrt').attr({ checked: true });
        });
        $('#aspecial').click(function () {

            $('#aOneway').removeClass('active');
            $('#aRound').removeClass('active');
            $('#aspecial').addClass('active');
            $('#frtn').show();
            $('#rbtnfrtsf').attr({ checked: true });
        });
        //Buses
        $('#aOnewayb').click(function () {
            $('#aRoundb').removeClass('active');
            $('#aOnewayb').addClass('active');
            $('#divReturn').hide();
            $('#rbtnbst').attr({ checked: true });
            $('#rbtnbrt').attr({ checked: false });
            $('#return').hide();
        });
        $('#aRoundb').click(function () {
            $('#aRoundb').addClass('active');
            $('#aOnewayb').removeClass('active');
            $('#divReturn').show();
            $('#rbtnbst').attr({ checked: false });
            $('#rbtnbrt').attr({ checked: true });
            $('#return').show();
        });

        //hotels
        $('#aDomestich').click(function () {

            $('#aInternationalh').removeClass('active');
            $('#aDomestich').addClass('active');
            $('#aDomestic').attr({ checked: true });
            $('#aInternational').attr({ checked: false });
            $('#txthdc').val('');
            $('#hdnhdc').val('');
        });
        $('#aInternationalh').click(function () {
            $('#aDomestich').removeClass('active');
            $('#aInternationalh').addClass('active');
            $('#aInternational').attr({ checked: true });
            $('#aDomestic').attr({ checked: false });
            $('#txthdc').val('');
            $('#hdnhdc').val('');
        });
        // Cabs
        $('#aOutstation').click(function () {
            $('#aTransfer').removeClass('active');
            $('#aLocal').removeClass('active');
            $('#aOutstation').addClass('active');

            $('#amultyc').hide();
            $('#afullday').hide();
            $('#ahalfday').hide();
            $('#aairport').hide();
            $('#aareahotel').hide();
            $('#arailwaystation').hide();
            $('#aRoundc').show();
            $('#aonec').show();
            $('#pick').hide();
            $('#drop').hide();
            $('#cto').show();
            $('#rbtncott').attr({ checked: true });
            $('#rbtncltt').attr({ checked: false });
            $('#rbtncttt').attr({ checked: false });
            document.getElementById("rbtncst").nextSibling.data = " One-Way ";
            document.getElementById("rbtncrt").nextSibling.data = " Round-trip ";
            document.getElementById("ddlcd").disabled = true;
            $('#txtcfcb').val('');
            $('#txtctcb').val('');

        });
        $('#aRoundc').click(function () {
            document.getElementById("ddlcd").disabled = false;
            $('#aonec').removeClass('active');
            //$('#amultyc').removeClass('active');
            $('#aRoundc').addClass('active');
            $('#pickdrop').hide();
            $('#rbtncst').attr({ checked: false });
            $('#rbtncrt').attr({ checked: true });
        });
        $('#aonec').click(function () {
            document.getElementById("ddlcd").disabled = true;
            $('#aonec').addClass('active');
            $('#amultyc').removeClass('active');
            $('#aRoundc').removeClass('active');
            $('#pickdrop').hide();
            $('#rbtncst').attr({ checked: true });
            $('#rbtncrt').attr({ checked: false });
        });

        $('#afullday').click(function () {
            document.getElementById("ddlcd").disabled = false;
            $('#afullday').addClass('active');
            $('#ahalfday').removeClass('active');


        });
        $('#ahalfday').click(function () {
            document.getElementById("ddlcd").disabled = false;
            $('#afullday').removeClass('active');
            $('#ahalfday').addClass('active');

        });
        $('#aairport').click(function () {
            document.getElementById("ddlcd").disabled = false;
            $('#aairport').addClass('active');
            $('#aareahotel').removeClass('active');
            $('#arailwaystation').removeClass('active');
            $('#rbtncrt').attr({ checked: false });
            $('#rbtncmct').attr({ checked: false });
            $('#rbtncst').attr({ checked: true });

        });
        $('#arailwaystation').click(function () {
            document.getElementById("ddlcd").disabled = false;
            $('#aairport').removeClass('active');
            $('#aareahotel').removeClass('active');
            $('#arailwaystation').addClass('active');
            $('#rbtncst').attr({ checked: false });
            $('#rbtncmct').attr({ checked: false });
            $('#rbtncrt').attr({ checked: true });

        });
        $('#aareahotel').click(function () {
            document.getElementById("ddlcd").disabled = false;
            $('#aairport').removeClass('active');
            $('#aareahotel').addClass('active');
            $('#arailwaystation').removeClass('active');
            $('#rbtncst').attr({ checked: false });
            $('#rbtncrt').attr({ checked: false });
            $('#rbtncmct').attr({ checked: true });

        });
        $('#amultyc').click(function () {
            document.getElementById("ddlcd").disabled = false;
            $('#aonec').removeClass('active');
            $('#amultyc').addClass('active');
            $('#aRoundc').removeClass('active');
        });
        $('#aLocal').click(function () {
            $('#aTransfer').removeClass('active');
            $('#aLocal').addClass('active');
            $('#aOutstation').removeClass('active');
            $('#rbtncott').attr({ checked: false });
            $('#rbtncltt').attr({ checked: true });
            $('#rbtncttt').attr({ checked: false });
            $('#amultyc').hide();
            $('#cto').hide();
            document.getElementById("ddlcd").disabled = false;
            //document.getElementById("rbtncst").nextSibling.data = " Full day ";
            //document.getElementById("rbtncrt").nextSibling.data = " Half day ";
            $('#afullday').show();
            $('#ahalfday').show();
            $('#aonec').hide();
            $('#aRoundc').hide();
            $('#aairport').hide();
            $('#arailwaystation').hide();
            $('#aareahotel').hide();
            $('#pick').hide();
            $('#drop').hide();
            $('#txtcfcb').val('');
            $('#txtctcb').val('');

        });

        $('#aTransfer').click(function () {
            $('#aOutstation').removeClass('active');
            $('#aTransfer').addClass('active');
            $('#aLocal').removeClass('active');
            $('#rbtncott').attr({ checked: false });
            $('#rbtncltt').attr({ checked: false });
            $('#rbtncttt').attr({ checked: true });
            $('#amultyc').hide();
            $('#afullday').hide();
            $('#ahalfday').hide();
            $('#cto').hide();
            document.getElementById("ddlcd").disabled = false;
            //document.getElementById("rbtncst").nextSibling.data = " Airport ";
            //document.getElementById("rbtncrt").nextSibling.data = " Railway station ";
            //document.getElementById("rbtncmct").nextSibling.data = " Area / Hotel ";
            $('#afullday').hide();
            $('#ahalfday').hide();
            $('#aonec').hide();
            $('#aRoundc').hide();
            $('#aairport').show();
            $('#arailwaystation').show();
            $('#aareahotel').show();
            $('#pick').show();
            $('#drop').show();
            $('#txtcfcb').val('');
            $('#txtctcb').val('');


        });

        function dspffnchangelocal(id) {

            $('#pick').hide();
            $('#drop').hide();
            $("#rbtncmct").hide();
            $("#cto").hide();
            $("#drop").hide();
            $('#txtcfcb').val('');
            $('#txtctcb').val('');


        }
        function dspffnchangetransfer(id) {

            $("#rbtncmct").show();
            $('#pick').show();
            $('#drop').show();
            $("#cto").hide();

            $('#txtcfcb').val('');
            $('#txtctcb').val('');

        }
        function dspffnchangeout(id) {
            $("#rbtncmct").hide();

            $('#cto').show();
            $('#txtcfcb').val('');
            $('#txtctcb').val('');
            $('#pick').hide();
            $('#drop').hide();

        }

    </script>
