<script type="text/javascript">
        if (window.location.pathname.indexOf('/') > -1) {
            var service = window.location.pathname.split('/');

            if (service[1].indexOf('SearchFlights') > -1) {

                sflights();

            } else if (service[1].indexOf('SearchHotels') > -1) {

                shotels();
            }
            else if (service[1].indexOf('SearchBuses') > -1) {
                sbuses();
            }
            else if (service[1].indexOf('SearchCabs') > -1) {
                scars();
            }
            else if (service[1].indexOf('SearchHolidays') > -1) {
                sholidays();
            }
            else if (service[1].indexOf('Recharge') > -1) {
                srecharge();
            }

        }
    </script>
